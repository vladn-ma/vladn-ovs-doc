ssh -X bldv-and-01

setenv PATH /home.local/vladn/emulation/qemu/bin:/tools/bin:/usr/bin:/bin

#linux:
cd /home.local/vladn/emulation/busybox/busybox-1.16.0
qemu-system-arm -M vexpress-a9 -kernel /home.local/vladn/emulation/linux/linux-3.2/arch/arm/boot/zImage -initrd rootfs.img.gz -display none -serial stdio -append "root=/dev/ram rdinit=/sbin/init console=ttyAMA0"


#uKernel:
cd /home.local/vladn/emulation/ukernel.old
qemu-system-arm -cpu any -M brcm-cmic -display none -kernel BCM56440_A0_0.elf





